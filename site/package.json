    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.2.1",
    "rollup": "^3.9.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-size": "^0.3.1",
